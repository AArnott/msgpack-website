<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="keywords" content="MessagePack,msgpack,JSON,alternative,better json,faster json,serializer">
    <link href='favicon.ico' rel='icon' type='image/x-icon'>

    <link rel="alternate" hreflang="en" href="index.html">
    <link rel="alternate" hreflang="ja" href="ja.html">

    <title>MessagePack Projects - {{ lang }}</title>

    <link media="all" rel="stylesheet" href="css/yuibase.css">
    <link media="all" rel="stylesheet" href="js/prettify/prettify.css">
    <link media="all" rel="stylesheet" href="css/all.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Titillium+Web:400,200">
    <script src="js/all.js"></script>
    <script src="js/prettify/prettify.js"></script>
    <script src="js/prettify/run_prettify.js"></script>
    <script src="js/jquery-2.1.4.min.js"></script>
    <script src="js/jquery.leanModal.min.js" ></script>
    <script src="js/msgpack.js" ></script>
  </head>
  <body>
    <header>
      <h1>MessagePack Projects ({{ lang }})</h1>
    </header>
    <main>
      {% if repos.length == 0 %}
        <p>No projects found for {{ lang }}.</p>
      {% else %}
        {% for repo in repos %}
          <article class="project">
            <h2><a href="{{ repo.html_url }}">{{ repo.full_name }}</a></h2>
            {% if repo.msgpack_repo_homepage %}
              <p>Homepage: <a href="{{ repo.msgpack_repo_homepage }}">{{ repo.msgpack_repo_homepage }}</a></p>
            {% endif %}
            <section class="quickstart">
              {{ repo.msgpack_quickstart_html | safe }}
            </section>
          </article>
        {% endfor %}
      {% endif %}
    </main>

    <footer>
      <p>Generated by modernized TypeScript toolchain.</p>
    </footer>
  </body>
</html>
